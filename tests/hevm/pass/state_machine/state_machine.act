constructor of StateMachine
interface constructor()

creates
  uint256 x := 0

invariants
    x <= 2

behaviour f of StateMachine
interface f()

iff

  x == 0
  CALLVALUE == 0

storage
    x => 1

behaviour g of StateMachine
interface g()

iff

  CALLVALUE == 0
  x == 1

  storage
    x => 2

behaviour h of StateMachine
interface h()

iff
  CALLVALUE == 0
  x == 2

  storage
    x => 0
